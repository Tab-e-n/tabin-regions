script/source = "extends Node

var connections : Array = [
#	Basic Connections are defined as such:
#	[\"Region 1\", \"Region 2\"],
#	Connections that are harder to traverse are defined as such:
#	[\"Hilly Region 1\", \"Hilly Region 2\", 1],
	[\"Line\", \"Dot\"],
	[\"Line\", \"Gigas\"],
	[\"Gigas\", \"Dot\"],
	[\"Gigas\", \"Swordsman\"],
	[\"Swordsman\", \"Dot\"],
	[\"Gigas\", \"Estro\"],
	[\"Flywrench\", \"Estro\"],
	[\"Flywrench\", \"D\"],
	[\"D\", \"Estro\"],
	[\"D\", \"Swordsman\"],
	[\"Swordsman\", \"Estro\"],
	[\"Oh\", \"Dot\"],
	[\"Line\", \"Oh\"],
	[\"Line\", \"Underline\"],
	[\"Underline\", \"Oh\"],
	[\"Arch\", \"Oh\"],
	[\"Arch\", \"Underline\"],
	[\"Arch\", \"Spiral\"],
	[\"Spiral\", \"Underline\"],
	[\"Arch\", \"Snake\"],
	[\"Spiral\", \"Snake\"],
]


func _ready():
	get_parent().connections = connections.duplicate()
	queue_free()

"

