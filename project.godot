config_version=5

[application]

config/name="Tabin's Regions"
config/description="A simple game about capturing regions."
run/main_scene="res://title.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="TRegs"
config/features=PackedStringArray("4.0", "GL Compatibility")
boot_splash/bg_color=Color(0.0745098, 0.054902, 0.0666667, 1)
boot_splash/show_image=false
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"

[autoload]

MapSetup="*res://Scripts/MapSetupData.gd"
GameStats="*res://Scripts/GameStats.gd"
ReplayControl="*res://Scripts/ReplayControler.gd"
