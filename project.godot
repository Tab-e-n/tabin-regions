[display]

window/size/viewport_height=640
window/size/mode=2
window/stretch/mode="canvas_items"

[filesystem]

import/blender/enabled=false

[gui]

theme/custom="res://Styles/default_theme.tres"
theme/custom_font="res://Fonts/Fredoka-Medium.ttf"
theme/default_font_antialiasing=0
